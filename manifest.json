{
  "name": "Jira Time Converter Extension",
  "version": "1.0",
  "description": "Convert dot time format to hour minute format.",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "<all_urls>"
  ]
}
